{
  "listen": 8000,
  "plugins": [
    "cache.js"
  ],
  "services": {
    "service-1": {
      "targets": [
        "127.0.0.1:8080"
      ]
    },
    "service-2": {
      "targets": [
        "127.0.0.1:8081",
        "127.0.0.1:8082"
      ]
    }
  },
  "routes": {
    "/*": {
      "service": "service-1"
    },
    "/hi/*": {
      "service": "service-2",
      "rewrite": [
        "^/hi",
        "/hello"
      ]
    }
  },
  "logURL": "http://127.0.0.1:8123/log"
}